  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "dev": "npm run start:debug",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "migration:run": "typeorm migration:run -d ./dist/providers/database/datasource.js",
    "migration": "npm run migration:run",
    "migration:revert": "typeorm migration:revert -d ./dist/providers/database/datasource.js",
    "migration:generate": "node ./dist/common/util/migration-generate.util.js",
    "migration:show": "typeorm migration:show -d ./dist/providers/database/datasource.js",
    "seed": "typeorm-seeding seed -r ./dist/database/seeders -c seeding-source.js -s BlogPostSeeder"
  },
  "dependencies": {
    "@concepta/typeorm-seeding": "^4.0.0",
    "@elastic/elasticsearch": "^8.19.1",
    "@fastify/cors": "^11.1.0",
    "@fastify/secure-session": "^8.2.0",
    "@keyv/redis": "^5.1.2",
    "@nestjs-modules/mailer": "^2.0.2",
    "@nestjs/bull": "^11.0.3",
    "@nestjs/cache-manager": "^3.0.1",
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.1",
    "@nestjs/elasticsearch": "^11.1.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.0.1",
    "@nestjs/platform-fastify": "^11.1.6",
    "cache-manager": "^7.2.0",
    "cache-manager-redis-store": "^3.0.1",
    "cache-manager-redis-yet": "^5.1.5",
    "cacheable": "^2.0.0",
    "class-validator": "^0.14.2",
    "dotenv": "^17.2.2",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "mysql2": "^3.14.5",
    "nestjs-paginate": "^12.8.2",
    "nodemailer": "^7.0.6",
    "passport": "^0.7.0",
    "passport-cookie": "^1.0.9",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
